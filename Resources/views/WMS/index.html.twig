{% extends 'MBWMSBundle:WMS:base.html.twig' %} 

{% block heading %}WMS{% endblock %}
{% block body %}

<form class="search wms_search">
    <div><label>{% trans %}Title{% endtrans %}<input name="title" /></label></div> 
    <input type="submit" value="{% trans %}Search{% endtrans %}" />
</form>

<ul class="wms">
    {% for wms in wmsArr %}
        <li><a href=" {{ path("mb_wms_details",{'id': wms.getId()}) }}">{{ wms.getTitle() }}</a></li>
    {% endfor %}
</ul>
<a href="{{ path("mb_wms_index",{"first":prevFirst}) }}">{% trans %}Previous{% endtrans %}</a> <a href="{{  path("mb_wms_index",{"first": nextFirst}) }}">{% trans %}Next{% endtrans %}</a>
{% endblock %}
