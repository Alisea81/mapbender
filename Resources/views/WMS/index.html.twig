{% extends 'MapbenderWmsBundle:WMS:base.html.twig' %} 

{% block heading %}WMS all{% endblock %}

{% block content %}

    <table class="wms">
        <caption>{% trans %}Verf√ºgbare WMS-Dienste{% endtrans %}</caption>
        <thead>
            <tr>
                <th>{% trans %}Title{% endtrans %}</th>
                <th>{% trans %}Alias{% endtrans %}</th>
                <th>{% trans %}Abstract{% endtrans %}</th>
            </tr>
        </thead>
        <tbody>
        {% for wms in wmsArr %}
            <tr>
                <td><a href="{{ path("mb_wms_wms_edit",{'wmsId': wms.id }) }}">{{ wms.title }}</a></td>
                <td>{{ wms.alias }}</td>
                <td>{{ wms.abstract }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
    <a href="{{ path("mb_wms_wms_index",{"first":prevFirst}) }}">{% trans %}Previous{% endtrans %}</a> 
    <a href="{{  path("mb_wms_wms_index",{"first": nextFirst}) }}">{% trans %}Next{% endtrans %}</a>
{% endblock %}
