{% extends "MapbenderCoreBundle:Group:base.html.twig" %}
{% form_theme form 'MapbenderCoreBundle:Form:fields.html.twig' %}

{% block content %}
  <form action="{{path("mapbender_core_group_add")}}" method="post">
    {{form_widget(form)}}
    <input type="submit" name="create" value="{{"Save"|trans}}" />
  </form>
  <form action="{{path("mapbender_core_group_index")}}" method="get">
    <input type="submit" name="create" value="{{"Cancel"|trans}}" />
  </form>
{% endblock %}
