{% extends '::base.html.twig' %}

{% block baseHead %}
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <!--[if IE]>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <![endif]-->
    <title>{% block title %}{% endblock %} - {{ server_name }}</title>

    {% block head %}{% endblock %}

    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/mapbendercore/reset.css') }}" />
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/mapbendercore/backend.css') }}" />
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/mapbendercore/mapquery/lib/jquery/themes/base/jquery-ui.css') }}" />
    <script src="{{ asset('bundles/mapbendermanager/js/modernizr-2.0.6.min.js') }}"></script>

    {% block css %}{% endblock %}
{% endblock %}

{% block baseContent %}
    <header>
    <h1><img alt="{{ server_name }}" src="{{ asset('bundles/mapbendercore/image/mapbender.png') }}"></h1>
    </header>

    {% block subnavigation %}{% endblock %}
    {% block content %}{% endblock %}

    <script type="text/javascript" src="{{ asset('bundles/mapbendercore/mapquery/lib/jquery/jquery-1.4.4.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/mapbendercore/mapquery/lib/jquery/ui/jquery-ui.js') }}"></script>

    {% block js %}{% endblock %}
{% endblock %}

