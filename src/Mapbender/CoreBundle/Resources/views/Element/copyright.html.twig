<span id="{{ id }}" class="mb-element mb-element-copyright {{configuration.anchor}}"  title="{{ configuration.tooltip|default(title)|trans }}">
    <span class="smallText span-copyright" title="">{{ configuration.copyright_text |trans |raw }}</span>
    {% if configuration.dialog_link is defined and configuration.dialog_link != ''%}
    <span id="{{ id }}-link" class="smallText mb-element-copyright-link">{{ configuration.dialog_link |trans |raw }}</span>
    <div id="{{ id }}-dialog" class="mb-element-copyright-dialog" title="{{ configuration.dialog_title |trans |raw }}">
        <p class="mb-element-copyright-content">{{ configuration.dialog_content |trans |raw }}</p>
    </div>
    {% endif %}
</span>