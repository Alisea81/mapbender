<fieldset class="layer">
            <legend>{{ layer.title }}</legend>
            <dl class="dl-horizontal hidde">
                <dt>{{ "Name" | trans }}</dt>
                <dd>{{ layer.name }}</dd>
                <dt>{{ "Title" | trans }}</dt>
                <dd>{{ layer.title }}</dd>
                <dt>{{ "Abstract" | trans }}</dt>
                <dd>{{ layer.abstract }}</dd>
                <dt>{{ "Queryable" | trans }}</dt>
                <dd>{{ layer.queryable }}</dd>
                <dt>{{ "Cascaded" | trans }}</dt>
                <dd>{{ layer.cascaded }}</dd>
                <dt>{{ "Opaque" | trans }}</dt>
                <dd>{{ layer.opaque }}</dd>
                <dt>{{ "NoSubset" | trans }}</dt>
                <dd>{{ layer.noSubset }}</dd>
                <dt>{{ "FixedWidth" | trans }}</dt>
                <dd>{{ layer.fixedWidth }}</dd>
                <dt>{{ "FixedHeight" | trans }}</dt>
                <dd>{{ layer.fixedHeight }}</dd>
                <dt>{{ "LatlonBounds" | trans }}</dt>
                <dd>
                    {% if layer.latlonBounds != null %}
                    {{ layer.latlonBounds.srs }} 
                    {{ layer.latlonBounds.minx }},
                    {{ layer.latlonBounds.miny }},
                    {{ layer.latlonBounds.maxx }},
                    {{ layer.latlonBounds.maxy }}
                    {% endif %}
                </dd>
                <dt>{{ "BoundingBox" | trans }}</dt>
                <dd>
                    {% for bbox in layer.boundingBoxes %}
                        {% if not loop.first %}<br/>{% endif %}
                        {{ bbox.srs }} 
                        {{ bbox.minx }},
                        {{ bbox.miny }},
                        {{ bbox.maxx }},
                        {{ bbox.maxy }}
                    {% endfor %}
                </dd>
                <dt>{{ "Srs" | trans }}</dt>
                <dd>
                    {% for srs in layer.srs %}
                        {% if not loop.first %},{% endif %}{{ srs }}
                    {% endfor %}
                </dd>
                <dt>{{ "Styles" | trans }}</dt>
                <dd>
                    {% if layer.styles|length > 0 %}
                    {% for style in layer.styles %}
                        {% if not loop.first %}<br/>{% endif %}
                        {{ "Name" | trans }}: {{ style.name }}<br/>
                        {{ "Title" | trans }}</dt>: {{ style.title }}<br/>
                        {{ "Abstract" | trans }}: {{ style.abstract }}<br/>
                        {{ "LegendUrl" | trans }}: 
                        {{ style.legendUrl.width }}X{{ style.legendUrl.height }}
                        {{ style.legendUrl.onlineResource.format }} {{ style.legendUrl.onlineResource.href }}<br/>
                        {{ "StyleSheetUrl" | trans }}:
                        {% if layer.styleSheetUrl is defined %}
                            {{ style.styleSheetUrl.format }} {{ style.styleSheetUrl.href }}
                        {% endif %}<br/>
                        {{ "StyleUrl" | trans }}:
                        {% if layer.styleUrl is defined %}
                            {{ style.styleUrl.format }} {{ style.styleUrl.href }}
                        {% endif %}
                    {% endfor %}
                    {% endif %}
                </dd>
                {% if layer.scale is defined and layer.scale != '' %}
                <dt>{{ "Scale" | trans }}</dt>
                <dd>
                    {{ "Min" | trans }}:{{ layer.scale.min }}<br/>
                    {{ "Max" | trans }}:{{ layer.scale.max }}
                </dd>
                {% endif %}
                {% if layer.scaleHint is defined and layer.scaleHint != '' %}
                <dt>{{ "ScaleHint" | trans }}</dt>
                <dd>
                    {{ "Min" | trans }}:{{ layer.scaleHint.min }}<br/>
                    {{ "Max" | trans }}:{{ layer.scaleHint.max }}
                </dd>
                {% endif %}
                <dt>{{ "Attribution" | trans }}</dt>
                <dd>
                {% if layer.attribution is defined and layer.attribution != '' %}
                    {{ "Title" | trans }}:{{ layer.attribution.title }}<br/>
                    {{ "OnlineResource" | trans }}:
                    {{ layer.attribution.onlineResource }}<br/>
                    {{ "LogoUrl" | trans }}:
                    {{ layer.attribution.logoUrl.width }}X{{layer.attribution.logoUrl.height }}
                    {{ layer.attribution.logoUrl.onlineResource.format }} {{ layer.attribution.logoUrl.onlineResource.href }}
                {% endif %}
                </dd>
                <dt>{{ "Identifier" | trans }}</dt>
                <dd>
                {% if layer.identifier is defined  %}
                {% for identifier in layer.identifier %}
                    {% if identifier.authority is defined and identifier.authority != '' %}
                    {% if not loop.first %}<br/>{% endif %}
                    {{ "Authority" | trans }}: 
                    {{ "Url" | trans }} {{ identifier.authority.url }}
                    {{ "Name" | trans }} {{ identifier.authority.name }}<br/>
                    {% endif %}
                    {% if identifier.value is defined %}
                    {{ "Value" | trans }}: {{ identifier.value }}<br/>
                    {% endif %}
                {% endfor %}
                {% endif %}
                </dd>
                <dt>{{ "Authority" | trans }}</dt>
                <dd>
                    {% if layer.authority is defined %}
                    {% for authority in layer.authority %}
                    {{ "Url" | trans }} {{ authority.url }}
                    {% if authority.name is defined %}
                    {{ "Name" | trans }} {{ authority.name }}<br/>
                    {% endif%}
                    {% endfor %}
                    {% endif%}
                </dd>
                <dt>{{ "MetadataUrl" | trans }}</dt>
                <dd>
                    {% if layer.metadataUrl is defined %}
                    {% for metadataUrl in layer.metadataUrl %}
                        {% if not loop.first %}<br/>{% endif %}
                        {{ metadataUrl.type }} {{ metadataUrl.onlineresource.format }} {{ metadataUrl.onlineresource.href }}
                    {% endfor %}
                    {% endif%}
                </dd>
                <dt>{{ "Dimension" | trans }}</dt>
                <dd>
                    {% if layer.dimension is defined %}
                    {% for dimension in layer.dimension %}
                        {% if not loop.first %}<br/>{% endif %}
                        {{ dimension.name }} {{ dimension.units }} {{ dimension.unitSymbol }}
                        {{ dimension.default }} {{ dimension.multipleValues }} {{ dimension.nearestValue }}
                        {{ dimension.current }} {{ dimension.extentValue }}
                    {% endfor %}
                    {% endif%}
                </dd>
                <dt>{{ "Extent" | trans }}</dt>
                <dd>
                    {% if layer.extent is defined %}
                    {% for extent in layer.extent %}
                        {% if not loop.first %}<br/>{% endif %}
                        {{ extent.name }} {{ extent.default }} {{ extent.multipleValues }}
                        {{ extent.nearestValue }} {{ extent.current }} {{ extent.extentValue }}
                    {% endfor %}
                    {% endif%}
                </dd>
                <dt>{{ "DataUrl" | trans }}</dt>
                <dd>
                    {% if layer.dataUrl is defined %}
                    {% for dataUrl in layer.dataUrl %}
                        {% if not loop.first %}<br/>{% endif %}
                        {{ dataUrl.format }} {{ dataUrl.href }}
                    {% endfor %}
                    {% endif%}
                </dd>
                <dt>{{ "FeatureListUrl" | trans }}</dt>
                <dd>
                    {% if layer.featureListUrl is defined %}
                    {% for featureUrl in layer.featureListUrl %}
                        {% if not loop.first %}<br/>{% endif %}
                        {{ featureUrl.format }} {{ featureUrl.href }}
                    {% endfor %}
                    {% endif%}
                </dd>
                <dt>{{ "Keywords" | trans }}</dt>
                <dd>
                    {#% if layer.keywords is defined and layer.keywords != null%}
                    {% for keyword in layer.keywords %}
                        {% if not loop.first %},{% endif %}{{ keyword.value }}
                    {% endfor %}
                    {% endif%#}
                </dd>
            </dl>
            {% if layer.sublayer|length > 0 %}
            {% for sublayer in layer.sublayer %}
                {% include "MapbenderWmsBundle:Repository:layer.html.twig"  with {'layer': sublayer }%}
            {% endfor %}
            {% endif %}
        </fieldset>